package br.com.tcd.enumeration;

/**
 *
 * @author Gabriela Mello
 */
public enum QuantidadeParafuso {

		ESCOLHA_QUANTIDADE_PARAFUSO("Escolha a quantidade de parafusos"),
		DOIS("2"),
		QUATRO("4"),
		CINCO("5"),
		SEIS("6"),
		OITO("8"),
		DEZ("10"),
		QUATORZE("14");

	private String nome;

	private QuantidadeParafuso(String nome) {
		this.nome = nome;

	}

	public String getNome() {
		return nome;
	}

	@Override
	public String toString() {
		return this.nome;
	}

}
