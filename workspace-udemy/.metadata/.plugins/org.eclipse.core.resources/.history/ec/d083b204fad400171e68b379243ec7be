package br.com.tcd.enumeration;

/**
 *
 * @author Gabriela Mello
 */
public enum ClasseAcoParafuso {
	
	ESCOLHA_CLASSE_ACO("Escolha a Classe de AÃ§o",0,0),
    BAIXO_TEOR("ISO 898-1 - Classe 4.6",235,400),
	BAIXO_TEOR("ISO 898-1 - Classe 8.8",640,800),
	BAIXO_TEOR("ISO 898-1 - Classe 10.9",900,1000);
    
    private String nome;
    private int fyk;
    private int fuk;

	private ClasseAcoParafuso(String nome, int fyk, int fuk) {
		this.nome = nome;
		this.fyk = fyk;
		this.fuk = fuk;
	}

	public String getNome() {
		return nome;
	}

	public static AcoParafuso getBaixoTeor() {
		return BAIXO_TEOR;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public int getFyk() {
		return fyk;
	}

	public void setFyk(int fyk) {
		this.fyk = fyk;
	}

	public int getFuk() {
		return fuk;
	}

	public void setFuk(int fuk) {
		this.fuk = fuk;
	}

	@Override
	public String toString() {
		return this.nome;
	}
    
}
