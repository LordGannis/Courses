package br.com.tcd.enumeration;

/**
 *
 * @author Gabriela Mello
 */
public enum ChapaAco {
	ESCOLHA_CLASSE_MADEIRA("Escolha a Classe da Madeira",0,0,0,0),
    C20("C-20",20,500,4,3500),
    C25("C-25",25,550,5,8500),
    C30("C-30",30,600,6,14500),
    D20("D-20",20,650,4,9500),
    D30("D-30",30,800,5,14500),
    D40("D-40",40,950,6,19500),
    D50("D-50",50,970,7,22000),
    D60("D-60",60,1000,8,24500);
	
//	"Escolha a espessura", "1/4\"     (6,35mm)", "5/16\"   (8,00 mm)", "3/8\"     (9,53 mm)", "1/2\"     (12,70 mm)",
//    "5/8\"     (15,90 mm)", "3/4\"     (19,10 mm)", "7/8\"     (22,20 mm)", "1\"        (25,40 mm)", "1 1/4\"  (31,80 mm)",
//    "1 1/2\"  (38,10 mm)", "1 5/8\"  (41,30 mm)", "1 3/4\"  (44,50 mm)", "2\"        (50,80 mm)", "2 1/4\"  (57,20 mm)",
//    "2 1/2\"  (63,50 mm)", "3\"        (76,20 mm)", "3 1/2\"  (88,90 mm)", "4\"        (102,00 mm)"
    
    private String nome;
    private int fc0k;
    private int Densidade;
    private int fv0k;
    private int ec0m;
    
    private ChapaAco (String nome,int fc0k, int Densidade, int fv0k, int ec0m){
        this.nome = nome;
        this.fc0k = fc0k;
        this.Densidade = Densidade;
        this.fv0k = fv0k;
        this.ec0m = ec0m;
        
}
    
    public String getNome() {
        return nome;
    }
    
    public int getfc0k(){
        return this.fc0k;}
   
    public int getDensidade(){
        return this.Densidade;}
    
    public int getfv0k(){
        return this.fv0k;}
    
    public int getec0m(){
        return this.ec0m;}

    @Override
    public String toString() {
        return this.nome;
    }
    
    
    
}
